    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.6",
    "@reduxjs/toolkit": "^2.9.2",
    "axios": "^1.13.2",
    "expo": "^54.0.24",
    "expo-auth-session": "~7.0.9",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
